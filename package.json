{
  "name": "DeckyCloudSync",
  "version": "1.0.14-dev",
  "description": "Decky plugin te mplate",
  "type": "module",
  "scripts": {
    "format": "prettier --write ./src",
    "lint": "eslint ./src --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --cache",
    "build": "pnpm format && pnpm lint && shx rm -rf dist && rollup -c --silent",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Emiliopg91/decky-plugin-template.git"
  },
  "keywords": [
    "decky",
    "plugin",
    "template"
  ],
  "author": "Emiliopg91",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/Emiliopg91/decky-plugin-template/issues"
  },
  "devDependencies": {
    "@decky/api": "^1.1.2",
    "@decky/rollup": "^1.0.1",
    "@decky/ui": "^4.7.2",
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^8.5.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/lodash": "^4.17.7",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/webpack": "^5.28.5",
    "decky-plugin-framework": "^1.4.8",
    "eslint": "8.57.1",
    "eslint-plugin-react": "^7.34.3",
    "prettier": "3.3.3",
    "rollup": "^4.21.3",
    "rollup-plugin-import-assets": "^1.1.1",
    "shx": "^0.3.4",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "async-mutex": "^0.5.0",
    "lodash": "^4.17.21",
    "react-icons": "^4.12.0"
  },
  "devRemoteBinaries": [
    {
      "url": "https://downloads.rclone.org/v1.70.2/rclone-v1.70.2-linux-amd64.zip",
      "sha256hash": "7bfa813f426820d20436e48a4e0b9bf21977fcd513a48f2d28eede3136b4bacb",
      "name": "backend/rclone.zip"
    }
  ],
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "decky-frontend-lib"
      ]
    }
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "packageManager": "pnpm@9.12.3+sha512.cce0f9de9c5a7c95bef944169cc5dfe8741abfb145078c0d508b868056848a87c81e626246cb60967cbd7fd29a6c062ef73ff840d96b3c86c40ac92cf4a813ee"
}
